{
  "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "pic_logo.png",
  "commands": [
    {
      "script": "__Script.js",
      "handler": "onRun",
      "shortcut": "shift cmd p",
      "name": "读取当前图层颜色",
      "identifier": "com.tbcolorpool.readcolorvalue"
    },
    {
      "script": "__Script.js",
      "handler": "restoreLayer",
      "shortcut": "shift cmd '",
      "name": "(UE)一键还原",
      "identifier": "com.tbcolorpool.restorelayer"
    },
    {
      "script": "__TBColorPoolUIFramework_framework_TBColorPoolUIFramework.js",
      "handler": "showPanel",
      "shortcut": "shift cmd +",
      "name": "开启面板",
      "identifier": "com.tbcolorpool.showPanel"
    },
    {
      "script": "__TBColorPoolUIFramework_framework_TBColorPoolUIFramework.js",
      "handlers": {
        "actions": {
          "Startup": "onStartup",
          "SelectionChanged.finish": "readColor"
        }
      }
    }
  ],
  "menu": {
    "items": [
      "com.tbcolorpool.showPanel",
      "-",
      "com.tbcolorpool.readcolorvalue",
      "com.tbcolorpool.restorelayer"
    ],
    "title": "🎨Tieba-ColorPool"
  },
  "identifier": "com.tbcolorpool.sketch",
  "version": "2.1.6",
  "description": "v2.1.6 色板及取色完成，取色基于色板， 为使用色板设置的颜色将会读取失败；修复framework问题",
  "authorEmail": "yanshby@icloud.com",
  "name": "🎨Tieba-ColorPool",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/YanShby/tb_colorpool_plugin/master/.appcast.xml",
  "author": "Shen,Yan"
}